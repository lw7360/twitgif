{
  "name": "TwitGif",
  "description": "Convert Twitter \"gifs\" to real gifs.",
  "version": "0.2",
  "permissions": [
    "contextMenus",
    "*://*.twitter.com/*",
    "*://*.twimg.com/*" 
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "scripts": ["worker_proxy.js", "videoconverter.js/ffmpeg.js", "main.js"]
  },
  "web_accessible_resources": ["worker_proxy.html"],
  "manifest_version": 2
}
